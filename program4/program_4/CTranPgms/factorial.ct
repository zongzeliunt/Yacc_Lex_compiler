
main() : void
begin
    int I
    do 100 I = 1, 10
    	printInt(I)
        printString(" Factorial = ")
    	printInt(factorial(I))
        printLine()
100 continue
end

factorial(n) : int
    int n 
begin
    if( n .eq. 1 )
    begin
	return 1
    end
    else
    begin
	 return (n * factorial(n-1))
    end
end
